<Window x:Class="MyHAstTagBoard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyHAstTagBoard"
        mc:Ignorable="d"
        Title="My#TagBoard" Height="419" Width="738" MinHeight="300" MinWidth="800">
    <Grid Margin="0,0,0,0" Background="#FF172734">
        <ListView x:Name="RequestedEvents" Margin="10,60,10,10" RenderTransformOrigin="0.516,0.239">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <ComboBox x:Name="CategoriesBox" Margin="10,0,10,333" VerticalAlignment="Bottom" Height="22" SelectedValuePath="Key" DisplayMemberPath="Value"/>
        <Label x:Name="label" Content="Доступные мероприятия на текущий момент:" HorizontalAlignment="Left" Margin="10,2,0,0" VerticalAlignment="Top" Width="336" Foreground="#FFF3E9E1"/>

    </Grid>
</Window>
